function openActionsheet(id,is_high,callback){
    actionsheet.init({
        frameBounces:false,//当前页面是否弹动，（主要针对安卓端）

        buttons:['拍照','图库选择']
    },function(ret){
        if(ret.buttonIndex==1)
          var sourcetype='camera';
        if(ret.buttonIndex==2)
          var sourcetype='library';

       selectPic(id,sourcetype,is_high,function(ret,err){
          callback(ret,err);
        });
    })
}
function selectPic(id,sourcetype,is_high,callback)
{
  var targetwidth=612;
  var targetheight=816
  var quality=50;
  if(is_high==1)
  {
    targetwidth=2448;
    targetheight=3264;
    quality=80;
  }
  api.getPicture({
      sourceType: sourcetype,
      // sourceType: 'library',
      encodingType: 'jpg,png',
      mediaValue: 'pic',
      // destinationType: 'base64',
      allowEdit: false,
      quality:quality,
      destinationType: 'url',
      targetWidth:targetwidth,
      targetHeight:targetheight,
      saveToPhotoAlbum: false
  }, function(ret, err) {
      // alert(JSON.stringify(ret));
      if (ret.data=="") {
        var picstatus=0;
        var img_src='';
      }
      else
      {
          // alert(JSON.stringify(ret));
          var picstatus=1;
          var  img_src=ret.data;
      }
      callback({picstatus:picstatus,img_src:img_src},err);
  })
}

