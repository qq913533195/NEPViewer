function uploadActionsheetMul(id,is_high,upload_url,callback)
{
    openActionsheetMul(id,is_high,function(ret,err){
      // alert(JSON.stringify(ret));
      if(ret.picstatus)
      {
          uploadPicMul(id,ret,upload_url);
      }
    });
    // callback();
}

