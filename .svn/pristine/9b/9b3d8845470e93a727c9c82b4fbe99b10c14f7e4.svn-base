<body>
    <section class="aui-content ">
        <ul class="aui-list aui-media-list ">
           <li class="aui-list-item">
                <div class="aui-list-item-inner">
                    <div class="aui-list-item-label">
                        <span id="Email_Name">Email</span>
                    </div>
                    <div class="aui-list-item-input" >
                        <input class="aui-input" type="text"  name="Email" id="name"  required="required" validate_msg="Email is require;" >
                    </div>
                </div>
            </li>
            <!-- <li class="aui-list-item">
                <div class="aui-list-item-inner ">
                    <div class="aui-list-item-label">
                      登录密码
                    </div>
                    <div class="aui-list-item-input">
                        <input class="aui-input" type="password"  placeholder="" id="password"  name="password">
                    </div>
                </div>
            </li> -->
            <li class="aui-list-item">
                <div class="aui-list-item-inner">
                    <div class="aui-list-item-label">
                        <span id="Password_Name">Password</span>
                    </div>
                    <div class="aui-list-item-input" >
                      <input class="aui-input"  type="password"  name="Password" id="Password"  required="required" validate_msg="Password is require;" >                       
                    </div>
                </div>
            </li>

            <li class="aui-list-item">
                <div class="aui-list-item-inner">
                    <div class="aui-list-item-label">
                        <span id="Confirm_Password_Name">Confirm  Password</span>
                    </div>
                    <div class="aui-list-item-input" >                   
                        <input class="aui-input" type="password"  name="pwd2" id="pwd2"  required="required" validate_msg="Password does not match;"  >
                    </div>
                </div>
            </li>
            <li class="aui-list-item">
                <div class="aui-list-item-inner">
                    <div class="aui-list-item-label">
                        <span id="Street_Name">Street</span>
                    </div>
                    <div class="aui-list-item-input" >
                          <input class="aui-input" type="text"  name="Street" id="Street"  required="required"  validate_msg="Street is require;" >
                    </div>
                </div>
            </li>
           <li class="aui-list-item">
                <div class="aui-list-item-inner">
                    <div class="aui-list-item-label">
                        <span id="City_Name">City</span>
                    </div>
                    <div class="aui-list-item-input" >                       
                        <input class="aui-input" type="text"  name="City" id="City"  required="required" validate_msg="City is require;">                         
                    </div>
                </div>
            </li>
            <li class="aui-list-item">
                <div class="aui-list-item-inner">
                    <div class="aui-list-item-label">
                        <span id="Country_Name">Country</span>
                    </div>
                    <div class="aui-list-item-input" >
                        <select name="Country" id="Country" class="form-control" onchange= "changeCountry()" required="required"  validate_msg="Country is require;">
                            <option value="" selected='selected'>select</option>
                        </select>                        
                    </div>
                </div>
            </li>
            <li class="aui-list-item">
                <div class="aui-list-item-inner">
                    <div class="aui-list-item-label">
                        <span id="State_Province">State</span>
                    </div>
                    <div class="aui-list-item-input" >
                        <select name="State" id="State" class="form-control" required="required"  validate_msg="State is require;">
                            <option value="" selected='selected'>select</option>
                        </select>                        
                    </div>
                </div>
            </li>
            <li class="aui-list-item">
                <div class="aui-list-item-inner">
                    <div class="aui-list-item-label">
                        <span id="Zip_Code">ZipCode</span>
                    </div>
                    <div class="aui-list-item-input" >                       
                        <input class="aui-input" type="text"  name="ZipCode" id="ZipCode"  required="required" validate_msg="ZipCode is require;">                         
                    </div>
                </div>
            </li>

            <li class="aui-list-item">
                <div class="aui-list-item-inner">
                    <div class="aui-list-item-label">
                        <span id="Contact_Person">Contact Person</span>
                    </div>
                    <div class="aui-list-item-input" >                       
                        <input class="aui-input" type="text"  name="Contact_Person" id="Contact_Person"  required="required" validate_msg="Contact_Person is require;">                         
                    </div>
                </div>
            </li>

            <li class="aui-list-item">
                <div class="aui-list-item-inner">
                    <div class="aui-list-item-label">
                        <span id="Contact_Number">Contact Number</span>
                    </div>
                    <div class="aui-list-item-input" >                       
                        <input class="aui-input" type="text"  name="Contact_Number" id="Contact_Number"  required="required" validate_msg="Contact_Number is require;">                         
                    </div>
                </div>
            </li>


            
         
        </ul>
            <div class="aui-margin-t-15 aui-padded-15">

                <div class="aui-btn  aui-btn-block aui-btn-info"  id="submit_button" onclick = upload()>Submit</div>

            </div>
    </section>


</body>
<script type="text/javascript" src="../script/api.js" ></script>
<script type="text/javascript" src="../js/common.js"></script>
<script type="text/javascript" src="../js/form_opration.js"></script>
<script type="text/javascript" src="../script/aui-dialog.js" ></script>
<script type="text/javascript" src="../script/aui-toast.js" ></script>

<script type="text/javascript">
    var mulimg_arr={};
    var dialog = new auiDialog();
    var toast = new auiToast({
    });
   var localization = $api.getStorage("localization");
  
    apiready = function(){
      
        initLabel();
        initForm();
        var localDir = api.wgtRootDir + '/res/json/';
        api.addEventListener({
            name: 'changeCountry'
        }, function(ret, err) {
          // 接口数据整理
            var country = ret.value.country; 
            api.readFile({
            path: localDir + 'state.json'
            }, function(ret, err) {
                if (ret.status) {
                
                    var options = '';
                    var state = JSON.parse(ret.data);
                    var data = state[country]
                    for (key in data) {         
                        var text=data[key];         
                        options += "<option value='"+key+"' >"+text+"</option>";              
                    }
                    $api.append($api.byId('State'), options);
                } else {
                    alert(err.msg);
                }
            });           

        });
    }
    function initForm()
    {
        var localDir = api.wgtRootDir + '/res/json/';
        api.readFile({
            path: localDir + 'country.json'
        }, function(ret, err) {
            if (ret.status) {
            
                var options = '';
                var data = JSON.parse(ret.data);
                for (key in data) {         
                    var text=data[key];         
                    options += "<option value='"+key+"' >"+text+"</option>";              
                }
                $api.append($api.byId('Country'), options);
            } else {
                 alert(err.msg);
            }
        });
       
    }

    function changeCountry() {
        var country = $api.val($api.byId('Country'));
        api.sendEvent({
            name: 'changeCountry',
            extra: {
               country: country
            }
        });
    }
    

    function setValue(form_id, value)
    {
      // if(api.pageParam.ids)
        $api.val($api.byId(form_id), value);
        // document.getElementById(form_id).value = value;

      // if(mainpart && mainpart[store_id])

    }

    function upload(){
        var rule = [{
            name: "Email",
            checkType: "email",          
            errorMsg: "Email is required;"
          },
          {
            name: "Password",       
            errorMsg: "Password is required;"
          },
          {
            name: "pwd2",
            errorMsg: "Password does not match;"
          },
          {
            name: "Street",
            errorMsg: "Street is required;"
          },
          {
            name: "City",
            errorMsg: "City is required;"
          },
          {
            name: "Country",
            errorMsg: "Country is required;"
          },
          {
            name: "State",
            errorMsg: "State is required;"
          },
          {
            name: "ZipCode",
            errorMsg: "ZipCode is required;"
          },
          {
            name: "Contact_Person",
            errorMsg: "Contact_Person is required;"
          },
          {
            name: "Contact_Number",           
            errorMsg: "Contact_Number is required;"
          },          
        ];
        var form_data= getForm();
        if(gracecheck(form_data, rule)){
            var data = {};
           
            var params_data = api.pageParam.data;
            data = Object.assign(form_data, params_data);
          
            var url=$api.getStorage('url') + '/register'
            // alert(JSON.stringify(data));
  
            ajaxUpload(url,{values: data},function(ret,err){
  
                if(ret.status)
                {
                    $api.setStorage('token', ret.data.token);                  
                    api.openWin({
                        name: 'index_win',
                        url: 'index_win.html',
                        slidBackEnabled:false
                    });

                    setTimeout(function() {
                        closeWin();
                    }, 1000);
        
                }
                else
                {
                    alert(ret.msg);
                }
                if(err)
                {
                    // senderror({uri:url,content:JSON.stringify(err)});
                //   alert(JSON.stringify(err));
                    alert('数据提交失败！');
  
                }
          })
      }

    }
    function initLabel()
    {
        var label_name = ['Email_Name','Password_Name', 'Confirm_Password_Name','Street_Name','City_Name','Country_Name','State_Province','Zip_Code','Contact_Person','Contact_Number','submit_button'];
        for(i=0;i<label_name.length;i++)
        {
            $api.html($api.byId(label_name[i]), localization[label_name[i]]);
        }
    }
</script>
</html>
